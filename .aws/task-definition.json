{
    "family": "vault-front-end-production", 
    "networkMode": "awsvpc", 

    "containerDefinitions": [
        {
            "name" : "vault-front-end-production",
            "cpu"      : 10,
            "memory"   : 256,
            "essential" : true,
            "portMappings": [
              {
                "containerPort": 80,
                "hostPort":  80
              },
                      {
                "containerPort" : 3000,
                "hostPort"      : 3000
              }
            ]
          }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
