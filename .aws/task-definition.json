{
    "family": "vault-front-end-production", 
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::account-id:role/ecs_task_execution_role",

    "containerDefinitions": [
        {
            "name" : "vault-front-end-production",
            "image" : "625116815523.dkr.ecr.us-east-1.amazonaws.com/vault-frontend:f395726759dbca1d9eb115d6303fd823f161221a",
            "cpu"      : 10,
            "memory"   : 256,
            "essential" : true,
            "portMappings": [
              {
                "containerPort": 80,
                "hostPort":  80
              },
                      {
                "containerPort" : 3000,
                "hostPort"      : 3000
              }
            ]
          }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
